      <p><strong>Bussit ilaaffissatit:</strong> {{ busTime }}</p> <!-- Show bus_time here -->\n      </div>\n\n      <!-- Confirm and Go Back buttons -->\n      <div class=\"field is-grouped mt-4\">\n        <div class=\"control\">\n          <button class=\"button is-primary\" @click=\"acceptEntry\">Akuerikkit</button> <!-- Button to accept entry -->\n        </div>\n        <div class=\"control\">\n          <button class=\"button is-light\" @click=\"goBack\">Uterit</button>\n        </div>\n        <div class=\"control\">\n          <button class=\"button is-danger\" @click=\"editEntry\">Edit Entry</button>\n          <!-- Edit button to delete and start over -->\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { useFormStore } from '../stores/formStore';\n\nexport default {\n  setup() {\n    const formStore = useFormStore();\n\n    // Access formData from the Pinia store\n    const formData = formStore.formData;\n\n    // Debugging: Log to check if data is being retrieved\n    console.log('Form Data:', formData);\n\n    return {\n      formData,\n    };\n  },\n  methods: {\n    acceptEntry() {\n      alert('Submission confirmed!');\n      this.$router.push({ name: 'HomePage' });\n    },\n    goBack() {\n      this.$router.go(-1);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.confirmation-form-container {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  padding: 20px;\n}\n\n.greyed-out-box {\n  background-color: #f0f0f0;\n  padding: 20px;\n  border-radius: 5px;\n}\n</style>\n"],"mappings":";AA0CA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,YAAW,QAAS,qBAAqB;AAElD,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIF,YAAY,CAAC,CAAC;;IAEhC;IACA,MAAMG,QAAO,GAAID,SAAS,CAACC,QAAQ;;IAEnC;IACAC,OAAO,CAACC,GAAG,CAAC,YAAY,EAAEF,QAAQ,CAAC;IAEnC,OAAO;MACLA;IACF,CAAC;EACH,CAAC;EACDG,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZC,KAAK,CAAC,uBAAuB,CAAC;MAC9B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE;MAAW,C